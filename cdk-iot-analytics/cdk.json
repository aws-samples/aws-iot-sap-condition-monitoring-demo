{
  "app": "python3 app.py",
  "context": {
    "@aws-cdk/core:enableStackNameDuplicates": "true",
    "aws-cdk:enableDiffNoFail": "true",
    "@aws-cdk/core:stackRelativeExports": "true",
    "@aws-cdk/aws-ecr-assets:dockerIgnoreSupport": true,
    "thing_name": "sap-iot-thing",
    "Type": "Compressor",
    "Equipment": "<SAP Equipment Number e.g. 217100091>",
    "FunctLoc": "<SAP Functional Location e.g. 1710-CWS-WCS-CWCP1>",
    "temperature_min": "12",
    "temperature_max": "14",
    "sns_alert_email_topic": "TemperatureAlarm",
    "alarm_emails": [
      "<name@domain.com>"
    ],
    "odpEntitySetName" : "NOTIF_CREATESet",
    "odpServiceName": "ZSERVICE_PM_NOTIFICATION_SRV",
    "sapHostName": "<host>",
    "sapPort": "<port>",
    "sapUsername": "<user>",
    "sapPassword": "<password>",
    "urlPrefix": "https://",
    "RSA_bitlength": 2048
  }
}
